{
  "background_color": "beige",
  "description": "Dra Hállyza | Ginecologia Regenarativa e Funcional & Saúde Vaginal.",
  "display": "fullscreen",
  "name": "Dra. Hállyza",
  "icons": [
    <% files = Dir.entries(Rails.root.join("app/assets/images/")).select {|f| !File.directory? f} %>
    <% files.each_with_index do |file, index| %>
      <% match = file.match(/.+-(?<size>\d{2,3}x\d{2,3}).png/) %>
      {
        "src": "<%= image_path "#{file}" %>",
        "sizes": "<%= match && match[:size] %>",
        "type": "image/png"
      }<%= "," unless (files.size - 1) == index %>
    <% end %>
  ]
}
